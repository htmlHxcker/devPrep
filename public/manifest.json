{
	"name": "Dev Prep",
	"version": "0.1.0",
	"author": "htmlHxcker",
	"manifest_version": 3,
	"description": "An extension to help you prepare for technical interviews with flashcards.",
	"background": {
		"service_worker": "./background.js"
	},
	"options_page": "options.html",
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "./images/devPrep16.png",
			"32": "./images/devPrepd32.png",
			"48": "./images/devPrep48.png",
			"128": "./images/devPrep128.png"
		}
	},
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"permissions": ["tabs", "storage", "contextMenus"],
	"icons": {
		"16": "./images/devPrep16.png",
		"32": "./images/devPrep32.png",
		"48": "./images/devPrep48.png",
		"128": "./images/devPrep128.png"
	}
}
